{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/digests/([^/]+)",
      "dest": "/api/digests/[id].js?id=$1"
    },
    {
      "src": "/api/digests/?$",
      "dest": "/api/digests/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
